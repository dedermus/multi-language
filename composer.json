{
    "name": "dedermus/multi-language",
    "description": "Expanding multilingual support for Laravel open-admin-core",
    "type": "library",
    "keywords": ["open-admin-core", "multi-language"],
    "homepage": "https://github.com/dedermus/multi-language",
    "license": "MIT",
    "authors": [
        {
            "name": "dedermus",
            "email": "dedermus@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "dedermus/open-admin-core": "^v2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdminCore\\Admin\\MultiLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdminCore\\Admin\\MultiLanguage\\MultiLanguageServiceProvider"
            ]
        }
    }
}
